{
    "source": {
        "directories": [
            "lib"
        ]
    },
    "logs": {
        "debug": "infection\/debug.log",
        "text": "infection\/infection.log"
    },
    "timeout": 30,
    "mutators": {
        "global-ignoreSourceCodeByRegex": [
            "assert.*"
        ],
        "@default": true,
        "IncrementInteger": {
            "ignoreSourceCodeByRegex": [
                ".*new.*Exception.*"
            ]
        },
        "DecrementInteger": {
            "ignoreSourceCodeByRegex": [
                ".*new.*Exception.*"
            ]
        }
    },
    "minMsi": 90,
    "minCoveredMsi": 90
}
